TERMUX

# 6x yes

pkg update -y && pkg upgrade -y && pkg install -y wget

cd ~/ && rm -f ~/nastavi-cc-ssh.sh && wget -q https://raw.githubusercontent.com/BLBMS/am-t/moje/0/nastavi-cc-ssh.sh \
&& chmod +x nastavi-cc-ssh.sh && ~/nastavi-cc-ssh.sh

cd ~/ && rm -f nastavi-cc-termux.sh && wget -q https://raw.githubusercontent.com/BLBMS/am-t/moje/0/nastavi-cc-termux.sh \
&& chmod +x nastavi-cc-termux.sh && ./nastavi-cc-termux.sh

restart

